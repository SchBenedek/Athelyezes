                <ListBoxItem>Csanád</ListBoxItem>
                <ListBoxItem>Drága</ListBoxItem>
                <ListBoxItem>Ember</ListBoxItem>
            </ListBox.Items>
        </ListBox>
        <Button Click="jobbra_Nyil" Content="-->" Background="LightBlue" HorizontalAlignment="Center" Margin="0,124,0,0" VerticalAlignment="Top"/>
        <Button Click="balra_Nyil" Content="&lt;--" Background="LightBlue" HorizontalAlignment="Center" VerticalAlignment="Center" Width="22"/>
        <ListBox Name="jobb_Lista" FontFamily="Times New Roman" FontSize="16" Height="NaN" Margin="488,73,175,106" Width="NaN">
            <ListBox.Items>
                <ListBoxItem>Lajos</ListBoxItem>
                <ListBoxItem>Nem</ListBoxItem>
                <ListBoxItem>Szép</ListBoxItem>
            </ListBox.Items>
        </ListBox>
        <Button Click="jobb_Elso" Content="Jobb első" Background="LightBlue" HorizontalAlignment="Left" Margin="683,109,0,0" VerticalAlignment="Top" Height="25" Width="73"/>
        <Button Click="jobb_Utolso" Content="Jobb utolsó" Background="LightBlue" HorizontalAlignment="Left" Margin="683,0,0,0" VerticalAlignment="Center" Height="25" Width="73"/>
        <Button Click="Hozzaadas" Content="Hozzáad" Background="LightBlue" HorizontalAlignment="Left" Margin="371,370,0,0" VerticalAlignment="Top"/>
        <TextBox Name="text_Hozzaad" FontFamily="Times New Roman" HorizontalAlignment="Left" Margin="336,318,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
