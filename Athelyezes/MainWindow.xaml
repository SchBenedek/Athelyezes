<Window x:Class="Athelyezes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Athelyezes"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Click="bal_Elso" Content="Bal első" Background="LightBlue" HorizontalAlignment="Left" Margin="65,114,0,0" VerticalAlignment="Top" Width="62"/>
        <Button Click="bal_Utolso" Content="Bal utolsó" Background="LightBlue" HorizontalAlignment="Left" Margin="65,0,0,0" VerticalAlignment="Center" Width="62"/>
        <ListBox Name="bal_Lista" FontFamily="Times New Roman" FontSize="16" Height="NaN" Margin="187,73,481,106" Width="NaN">
